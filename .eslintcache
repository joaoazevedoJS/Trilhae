Sucessed/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/pages/SignupSucessed/index.tsx",[],"/home/joao/Documents/hackathonCCR/web/src/pages/SignupSucessed/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Helcome/index.tsx",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Helcome/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Courses/index.tsx",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Courses/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Points/index.tsx",["208"],"import React, { FC } from 'react';\nimport Header from '../../../components/Header';\n\nimport TitleContentInfo from '../../../components/TitleContentInfo';\nimport Footer from '../../../components/Footer';\nimport Link from '../../../components/Link';\n\nimport { Page, CardPoint, Container } from './styles';\n\nconst Points: FC = () => {\n  return (\n    <Page>\n      <Container>\n        <Header>\n          <Link to=\"/logged/courses\">Cursos</Link>\n          <Link to=\"/logged/awards\">Prêmios</Link>\n          <Link selected to=\"/logged/points\">\n            Pontuação\n          </Link>\n        </Header>\n\n        <TitleContentInfo title=\"Pontuação\" big />\n\n        <div className=\"points\">\n          {['', '', '', '', '', '', ''].map((course, index) => (\n            <CardPoint key={index}>\n              <h3>Curso: Comunicação Eficaz</h3>\n\n              <p>Etapa: 01</p>\n              <p>10 pontos ganhos</p>\n              <p>Dia 17/08/2021</p>\n            </CardPoint>\n          ))}\n        </div>\n      </Container>\n\n      <Footer />\n    </Page>\n  );\n};\n\nexport default Points;\n","/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Points/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Awards/index.tsx",["209"],"import React, { FC, useCallback, useState } from 'react';\nimport Header from '../../../components/Header';\n\nimport TitleContentInfo from '../../../components/TitleContentInfo';\nimport Footer from '../../../components/Footer';\nimport Link from '../../../components/Link';\n\nimport TempImg from '../../../assets/images/temp/tempImg-2.jpg';\n\nimport { Page, CardAward, Container } from './styles';\nimport Button from '../../../components/Button';\nimport MessagePointModal from '../../../components/MessagePointModal';\n\nconst Awards: FC = () => {\n  const [openMessageModal, setOpenMessageModal] = useState({\n    open: false,\n    error: false,\n  });\n\n  const handleExchangePoint = useCallback((point: string) => {\n    setOpenMessageModal({ open: true, error: point === '0' });\n  }, []);\n\n  return (\n    <Page>\n      <Container>\n        <Header>\n          <Link to=\"/logged/courses\">Cursos</Link>\n\n          <Link selected to=\"/logged/awards\">\n            Prêmios\n          </Link>\n\n          <Link to=\"/logged/points\">Pontuação</Link>\n        </Header>\n\n        <TitleContentInfo title=\"Premios\" big />\n\n        <div className=\"points\">\n          {['1', '1', '0', '1', '1', '0', '1'].map((point, index) => (\n            <CardAward key={index} onClick={() => handleExchangePoint(point)}>\n              <img src={TempImg} alt=\"Award\" />\n\n              <div>\n                <h3>Lorem ipsum dolor</h3>\n\n                <p>100 pontos</p>\n\n                <Button uppercase>Trocar</Button>\n              </div>\n            </CardAward>\n          ))}\n        </div>\n      </Container>\n\n      {openMessageModal.open && (\n        <MessagePointModal\n          setState={setOpenMessageModal}\n          isErrored={openMessageModal.error}\n        />\n      )}\n\n      <Footer />\n    </Page>\n  );\n};\n\nexport default Awards;\n","/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Awards/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/components/Link/index.tsx",[],"/home/joao/Documents/hackathonCCR/web/src/components/Link/styled.ts",[],"/home/joao/Documents/hackathonCCR/web/src/components/MessagePointModal/index.tsx",[],"/home/joao/Documents/hackathonCCR/web/src/components/MessagePointModal/styles.ts",[],"/home/joao/Documents/hackathonCCR/web/src/pages/Logged/Classroom/index.tsx",["210","211"],"import React, { FC, useCallback, useState } from 'react';\nimport { useParams } from 'react-router-dom';\nimport { FiCircle, FiLock } from 'react-icons/fi';\n\nimport Footer from '../../../components/Footer';\nimport Header from '../../../components/Header';\nimport Link from '../../../components/Link';\n\nimport { Page, Container } from './styles';\nimport Button from '../../../components/Button';\n\ninterface ParamsProps {\n  course: string;\n}\n\nconst Classroom: FC = () => {\n  const params = useParams<ParamsProps>();\n  const [opensClass, setOpensClass] = useState([\n    {\n      open: true,\n      finished: false,\n    },\n    {\n      open: false,\n      finished: false,\n    },\n    {\n      open: false,\n      finished: false,\n    },\n    {\n      open: false,\n      finished: false,\n    },\n  ]);\n\n  const [questions, setQuestions] = useState([false, false, false]);\n\n  const handleOpenNewClass = useCallback(\n    (index: number) => {\n      opensClass[index] = {\n        open: true,\n        finished: true,\n      };\n\n      if (opensClass.length > index + 1) {\n        opensClass[index + 1] = {\n          open: true,\n          finished: false,\n        };\n      }\n\n      setOpensClass([...opensClass]);\n    },\n    [opensClass],\n  );\n\n  const handleSelectQuestion = useCallback(\n    (index: number) => {\n      const mapQuestions = questions.map((_, indexQ) => {\n        return indexQ === index;\n      });\n\n      setQuestions(mapQuestions);\n    },\n    [questions],\n  );\n\n  return (\n    <Page>\n      <Container>\n        <Header>\n          <Link to=\"/logged/courses\">Cursos</Link>\n          <Link to=\"/logged/awards\">Prêmios</Link>\n          <Link to=\"/logged/points\">Pontuação</Link>\n        </Header>\n\n        <div className=\"class\">\n          <h3>Curso {params.course}</h3>\n\n          {opensClass.map((openClass, index) => (\n            <button\n              key={index}\n              className=\"btn-select\"\n              type=\"button\"\n              onClick={() => openClass.open && handleOpenNewClass(index)}\n            >\n              Aula {index + 1}\n              {openClass.open && (\n                <FiCircle className={openClass.finished ? 'finished' : ''} />\n              )}\n              {!openClass.open && <FiLock />}\n            </button>\n          ))}\n        </div>\n\n        <div className=\"class-content\">\n          <iframe\n            title=\"class\"\n            width=\"100%\"\n            height=\"350px\"\n            src=\"https://www.youtube.com/embed/Uma65YCKxME\"\n            frameBorder=\"0\"\n            allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\"\n            allowFullScreen\n          />\n\n          <h3>\n            E aí, gostou? Responda a pergunta a seguir para desbloquear a\n            próxima etapa do seu curso.\n          </h3>\n\n          <p>\n            Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do\n            eiusmod tempor incididunt ut consectetur adipiscing elit?\n          </p>\n\n          {questions.map((question, index) => (\n            <button\n              className=\"btn-select\"\n              key={index}\n              type=\"button\"\n              onClick={() => handleSelectQuestion(index)}\n            >\n              <FiCircle className={question ? 'finished' : ''} />\n              Lorem ipsum dolor sit amet, consectetur adipiscing\n            </button>\n          ))}\n\n          <Button transparent blue>\n            Enviar\n          </Button>\n        </div>\n      </Container>\n\n      <Footer />\n    </Page>\n  );\n};\n\nexport default Classroom;\n","/home/joao/Documents/hackathonCCR/web/src/pages/Logged/C